{
  "name": "strategy-swappers",
  "version": "1.0.0",
  "description": "Yearn strategy swappers",
  "keywords": [
    "ethereum",
    "smart",
    "contracts",
    "test",
    "solidity",
    "hardhat",
    "yearn"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lbertenasco/strategy-swappers.git"
  },
  "scripts": {
    "commit": "git-cz",
    "compile": "hardhat compile",
    "coverage": "hardhat coverage",
    "fork:node": "FORK=true hardhat node",
    "fork:script": "FORK=true hardhat run",
    "fork:test": "FORK=true yarn test",
    "postinstall": "husky install",
    "lint:check": "solhint 'contracts/**/*.sol' 'interfaces/**/*.sol' && prettier --check './**'",
    "lint:fix": "sort-package-json && prettier --write './**' && solhint --fix 'contracts/**/*.sol' 'interfaces/**/*.sol'",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "release": "standard-version",
    "test": "TEST=true hardhat compile && TEST=true mocha",
    "test:e2e": "yarn test ./test/e2e/**/*.spec.ts",
    "test:integration": "yarn test ./test/integration/**/*.spec.ts",
    "test:gas": "REPORT_GAS=1 npx hardhat test",
    "test:unit": "yarn test 'test/unit/**/*.spec.ts'",
    "deploy:polygon": "npx hardhat deploy --tags Common,Polygon --network polygon",
    "deploy:fantom": "npx hardhat deploy --tags Common,Fantom --network fantom"
  },
  "dependencies": {
    "@nomiclabs/ethereumjs-vm": "4.2.2",
    "@openzeppelin/contracts": "4.3.1",
    "@uniswap/v2-periphery": "1.1.0-beta.0",
    "qs": "6.10.1"
  },
  "devDependencies": {
    "@commitlint/cli": "13.1.0",
    "@commitlint/config-conventional": "13.1.0",
    "@eth-optimism/smock": "1.1.10",
    "@lbertenasco/contract-utils": "0.3.8",
    "@nomiclabs/hardhat-ethers": "npm:hardhat-deploy-ethers@0.3.0-beta.10",
    "@nomiclabs/hardhat-etherscan": "2.1.6",
    "@nomiclabs/hardhat-waffle": "2.0.1",
    "@types/chai": "4.2.21",
    "@types/chai-as-promised": "7.1.4",
    "@types/lodash": "4.14.172",
    "@types/mocha": "9.0.0",
    "@types/moment": "2.13.0",
    "@types/node": "^16.9.0",
    "@uniswap/v2-core": "1.0.1",
    "@uniswap/v2-periphery": "1.1.0-beta.0",
    "@uniswap/v3-periphery": "1.1.1",
    "aws-sdk": "2.985.0",
    "axios": "0.21.4",
    "axios-retry": "3.1.9",
    "chai": "4.3.4",
    "chai-as-promised": "7.1.1",
    "dotenv": "10.0.0",
    "ethereum-waffle": "3.4.0",
    "ethers": "5.4.6",
    "hardhat": "2.6.3",
    "hardhat-deploy": "0.9.1",
    "hardhat-gas-reporter": "1.0.4",
    "hardhat-preprocessor": "0.1.4",
    "husky": "7.0.2",
    "inquirer": "8.1.2",
    "lodash": "4.17.21",
    "moment": "2.29.1",
    "pinst": "2.1.6",
    "prettier": "2.4.0",
    "prettier-plugin-solidity": "1.0.0-beta.17",
    "solhint": "3.3.6",
    "solhint-plugin-prettier": "0.0.5",
    "solidity-coverage": "0.7.17",
    "sort-package-json": "1.50.0",
    "standard-version": "9.3.1",
    "ts-node": "10.2.1",
    "typescript": "4.4.2"
  },
  "authors": [
    {
      "name": "Luciano Bertenasco",
      "email": "mail",
      "url": "https://github.com/lbertenasco"
    },
    {
      "name": "b0dhidharma",
      "email": "b0dhidharma@protonmail.com",
      "url": "https://github.com/b0dhidharma"
    }
  ]
}
